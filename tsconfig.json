{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["dom", "dom.iterable", "ES2020"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "ES2020",
      "moduleResolution": "Bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "@/public/*": ["./public/*"]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/assets/images/index.ts.bk", "src/components/MyPlayer.js", "src/lib/chat-utils/plugins/color.js"],
    "exclude": ["node_modules"]
  }
  